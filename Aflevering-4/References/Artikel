public class Artikel {
	String forfattere;
	Artikel[] referenceListe;
	String titel;
	Tidsskrift tidsskrift;

	public Artikel(String titel, String forfatterIn, Tidsskrift tidsskriftIn) {
		navn = navnIn;
		titel = titelIn;
		tidsskrift = tidsskriftIn;
	}

	public void setReferenceList(Artikel[] refListeIn) {
		referenceListe = refListeIn;
	}

	public String toString() {
		String output = "";

		for (int i = 0; i < referenceListe.length; i++) {
			output += navn + ": " + titel + ", " + tidsskrift + "\n";
		}

		return output;
	}
}